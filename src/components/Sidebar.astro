<aside id="side">
    <nav>
        <ul class="lexend-reg extend" id="list"></ul>
    </nav>
</aside>

<style>
    #list {
        list-style: none;
        padding-top: 0px;
        padding-left: 15px;
    }

    #side {
        line-height: 30px;
        width: 15vw;
        background-color: #131212;
        height: 100vh;
        padding-top: 1px;
    }

    .extend {
        position: fixed;
    }
</style>

<script>
const o = [
    {
        name: "Overview",
        href: "../../docs/guides"
    },
    {
        name: "UKMCL",
        href: "../../docs/ukmcl"
    },
    {
        name: "Unknown Mod",
        href: "../../docs/unkmod"
    },
    {
        name: "Custom Splashes",
        href: "../../docs/custom"
    },
    {
        name: "Elementa",
        href: "../../docs/elementa"
    }
];

const con = document.getElementById("list");

o.forEach(e => {
    const a = document.createElement("a");
    const li = document.createElement("li");
    a.href = e.href;
    a.textContent = e.name;
    a.className = "thing"
    li.appendChild(a);
    con?.appendChild(li);
})
</script>